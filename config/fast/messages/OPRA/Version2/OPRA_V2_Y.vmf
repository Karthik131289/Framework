<?xml version="1.0" encoding="UTF-8" ?>
<FastMessageFormat name="OPRA_V2_Y">
	<standard>FAST</standard>
	<standard-name>OPRA_V2_Y</standard-name>
	<DescriptiveName><![CDATA[OPRA_V2_Y]]></DescriptiveName>
	<Category>OPRA\Version2</Category>
	<Fastfields>
		<defaultencoding>
			<fastencoding>
				<dictionary>global</dictionary>
			</fastencoding>
		</defaultencoding>
		<headerfields>
		</headerfields>
		<datafields>
			<field>
				<name>MESSAGE_CATEGORY</name>
				<datatype>Character</datatype>
				<required>true</required>
				<fastencoding>
					<slot>0</slot>
					<fast-type>uInt32</fast-type>
					<encode-operator>copy</encode-operator>
					<id>0</id>
					<key>TID</key>
				</fastencoding>
			</field>
			<field>
				<name>MESSAGE_TYPE</name>
				<datatype>Character</datatype>
				<required>true</required>
				<fastencoding>
					<slot>1</slot>
					<fast-type>uInt32</fast-type>
					<encode-operator>copy</encode-operator>
					<id>1</id>
				</fastencoding>
			</field>
			<field>
				<name>PARTICIPANT_ID</name>
				<datatype>Character</datatype>
				<required>true</required>
				<fastencoding>
					<slot>2</slot>
					<fast-type>uInt32</fast-type>
					<encode-operator>copy</encode-operator>
					<id>2</id>
				</fastencoding>
			</field>
			<field>
				<name>RETRANSMISSION_REQUESTER</name>
				<datatype>Character</datatype>
				<required>true</required>
				<fastencoding>
					<slot>3</slot>
					<fast-type>uInt32</fast-type>
					<encode-operator>copy</encode-operator>
					<id>3</id>
				</fastencoding>
			</field>
			<field>
				<name>MESSAGE_SEQUENCE_NUMBER</name>
				<datatype>Integer</datatype>
				<required>true</required>
				<fastencoding>
					<slot>4</slot>
					<fast-type>uInt32</fast-type>
					<encode-operator>increment</encode-operator>
					<id>4</id>
				</fastencoding>
			</field>
			<field>
				<name>TIME</name>
				<datatype>Integer</datatype>
				<required>true</required>
				<fastencoding>
					<slot>5</slot>
					<fast-type>uInt32</fast-type>
					<encode-operator>copy</encode-operator>
					<id>5</id>
				</fastencoding>
			</field>
			<field>
				<name>group</name>
				<datatype>Section</datatype>
				<minoccurs>0</minoccurs>
				<maxoccurs>1</maxoccurs>
				<section-type>normal</section-type>
				<discriminator>MESSAGE_TYPE == ' '</discriminator>
				<fields>
					<field>
						<name>NUMBER_OF_INDICES_IN_GROUP</name>
						<datatype>Integer</datatype>
						<required>true</required>
						<fastencoding>
							<slot>39</slot>
							<fast-type>uInt32</fast-type>
							<encode-operator>copy</encode-operator>
							<id>39</id>
						</fastencoding>
					</field>
					<field>
						<name>indexGroup</name>
						<datatype>Section</datatype>
						<minoccurs>0</minoccurs>
						<maxoccurs>-1</maxoccurs>
						<section-type>normal</section-type>
						<discriminator>group.indexGroup.$size &lt; group.NUMBER_OF_INDICES_IN_GROUP</discriminator>
						<fields>
							<field>
								<name>INDEX_SYMBOL</name>
								<datatype>String</datatype>
								<required>true</required>
								<fastencoding>
									<slot>41</slot>
									<fast-type>string</fast-type>
									<encode-operator>copy</encode-operator>
									<id>41</id>
								</fastencoding>
							</field>
							<field>
								<name>INDEX_VALUE</name>
								<datatype>String</datatype>
								<required>false</required>
								<fastencoding>
									<slot>42</slot>
									<fast-type>string</fast-type>
									<encode-operator>copy</encode-operator>
									<id>42</id>
								</fastencoding>
							</field>
						</fields>
					</field>
				</fields>
			</field>
			<field>
				<name>bidOffer</name>
				<datatype>Section</datatype>
				<minoccurs>0</minoccurs>
				<maxoccurs>1</maxoccurs>
				<section-type>normal</section-type>
				<discriminator>MESSAGE_TYPE == 'I'</discriminator>
				<fields>
					<field>
						<name>NUMBER_OF_INDICES_IN_GROUP</name>
						<datatype>Integer</datatype>
						<required>true</required>
						<fastencoding>
							<slot>39</slot>
							<fast-type>uInt32</fast-type>
							<encode-operator>copy</encode-operator>
							<id>39</id>
						</fastencoding>
					</field>
					<field>
						<name>indexGroup</name>
						<datatype>Section</datatype>
						<minoccurs>0</minoccurs>
						<maxoccurs>-1</maxoccurs>
						<section-type>normal</section-type>
						<discriminator>bidOffer.indexGroup.$size &lt; bidOffer.NUMBER_OF_INDICES_IN_GROUP</discriminator>
						<fields>
							<field>
								<name>INDEX_SYMBOL</name>
								<datatype>String</datatype>
								<required>true</required>
								<fastencoding>
									<slot>41</slot>
									<fast-type>string</fast-type>
									<encode-operator>copy</encode-operator>
									<id>41</id>
								</fastencoding>
							</field>
							<field>
								<name>BID_INDEX_VALUE</name>
								<datatype>String</datatype>
								<required>true</required>
								<fastencoding>
									<slot>43</slot>
									<fast-type>string</fast-type>
									<encode-operator>copy</encode-operator>
									<id>43</id>
								</fastencoding>
							</field>
							<field>
								<name>OFFER_INDEX_VALUE</name>
								<datatype>String</datatype>
								<required>true</required>
								<fastencoding>
									<slot>44</slot>
									<fast-type>string</fast-type>
									<encode-operator>copy</encode-operator>
									<id>44</id>
								</fastencoding>
							</field>
						</fields>
					</field>
				</fields>
			</field>
			<field>
				<name>fcSpotVal</name>
				<datatype>Section</datatype>
				<minoccurs>0</minoccurs>
				<maxoccurs>1</maxoccurs>
				<section-type>normal</section-type>
				<discriminator>MESSAGE_TYPE == 'F' || MESSAGE_TYPE == 'C'</discriminator>
				<fields>
					<field>
						<name>NUMBER_OF_FOREIGN_CURRENCY_SPOT_VALUES_IN_GROUP</name>
						<datatype>Integer</datatype>
						<required>true</required>
						<fastencoding>
							<slot>40</slot>
							<fast-type>uInt32</fast-type>
							<encode-operator>copy</encode-operator>
							<id>40</id>
						</fastencoding>
					</field>
					<field>
						<name>indexGroup</name>
						<datatype>Section</datatype>
						<minoccurs>0</minoccurs>
						<maxoccurs>-1</maxoccurs>
						<section-type>normal</section-type>
						<discriminator>fcSpotVal.indexGroup.$size &lt; fcSpotVal.NUMBER_OF_FOREIGN_CURRENCY_SPOT_VALUES_IN_GROUP</discriminator>
						<fields>
							<field>
								<name>FCO_SYMBOL</name>
								<datatype>String</datatype>
								<required>true</required>
								<fastencoding>
									<slot>45</slot>
									<fast-type>string</fast-type>
									<encode-operator>copy</encode-operator>
									<id>45</id>
								</fastencoding>
							</field>
							<field>
								<name>DECIMAL_PLACEMENT_INDICATOR</name>
								<datatype>Integer</datatype>
								<required>true</required>
								<fastencoding>
									<slot>46</slot>
									<fast-type>uInt32</fast-type>
									<encode-operator>copy</encode-operator>
									<id>46</id>
									<dictionary></dictionary>
								</fastencoding>
							</field>
							<field>
								<name>FOREIGN_CURRENCY_SPOT_VALUE</name>
								<datatype>Integer</datatype>
								<required>true</required>
								<fastencoding>
									<slot>47</slot>
									<fast-type>uInt32</fast-type>
									<encode-operator>copy</encode-operator>
									<id>47</id>
									<dictionary></dictionary>
								</fastencoding>
							</field>
						</fields>
					</field>
				</fields>
			</field>
		</datafields>
		<trailerfields>
		</trailerfields>
	</Fastfields>
	<validationrules name="Default">
		<hdrvalidationrules>
			<fieldvalidations>
			</fieldvalidations>
		</hdrvalidationrules>
		<datavalidationrules>
			<fieldvalidations>
			</fieldvalidations>
		</datavalidationrules>
		<tlrvalidationrules>
			<fieldvalidations>
			</fieldvalidations>
		</tlrvalidationrules>
	</validationrules>
</FastMessageFormat>

